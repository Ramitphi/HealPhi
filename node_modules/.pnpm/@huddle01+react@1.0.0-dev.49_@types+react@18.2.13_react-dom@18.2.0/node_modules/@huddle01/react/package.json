{
  "name": "@huddle01/react",
  "version": "1.0.0-dev.49",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "pub": "npm publish --access public --tag dev"
  },
  "tsup": {
    "entry": ["src/**/*.ts"],
    "sourcemap": true,
    "dts": true,
    "splitting": true,
    "clean": true,
    "format": ["cjs", "esm"]
  },
  "files": ["dist/*", "index.d.ts", "hooks.d.ts"],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.20",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "types": "workspace:*",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@huddle01/web-core": "1.0.0-dev.66",
    "@tanstack/react-query": "^4.32.6",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "axios": "^1.5.1",
    "react": "18.2.0"
  }
}
